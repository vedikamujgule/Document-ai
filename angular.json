{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "cli": {
        "analytics": "ece86193-146e-4556-9a67-214881cd13f5"
    },
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "cadent": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/cadent",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.app.json",
                        "aot": false,
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "styles": [
                            "src/styles.scss",
                            "node_modules/bootstrap/scss/bootstrap.scss",
                            "node_modules/angular-lightgallery/dist/css/lightgallery.min.css",
                            "node_modules/filepond/dist/filepond.min.css",
                            "node_modules/filepond-plugin-image-preview/dist/filepond-plugin-image-preview.min.css"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "dev": {
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.dev.ts"
                            }],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [{
                                    "type": "initial",
                                    "maximumWarning": "6mb",
                                    "maximumError": "6mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "180kb",
                                    "maximumError": "180kb"
                                }
                            ]
                        },
                        "production": {
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.prod.ts"
                            }],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [{
                                    "type": "initial",
                                    "maximumWarning": "6mb",
                                    "maximumError": "6mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "cadent:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "cadent:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "cadent:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.spec.json",
                        "karmaConfig": "karma.conf.js",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "styles": [
                            "src/styles.scss",
                            "node_modules/angular-lightgallery/dist/css/lightgallery.min.css",
                            "node_modules/filepond/dist/filepond.min.css",
                            "node_modules/filepond-plugin-image-preview/dist/filepond-plugin-image-preview.min.css"
                        ],
                        "scripts": [
                            "node_modules/bootstrap/dist/bootstrap.min.js",
                            "node_modules/filepond/dist/filepond.min.js",
                            "node_modules/filepond-plugin-image-preview/dist/filepond-plugin-image-preview.min.js",
                            "node_modules/filepond-plugin-image-transform/dist/filepond-plugin-image-transform.min.js",
                            "node_modules/filepond-plugin-image-resize/dist/filepond-plugin-image-resize.min.js",
                            "node_modules/filepond-plugin-image-filter/dist/filepond-plugin-image-filter.min.js",
                            "node_modules/angular-lightgallery/dist/js/lightgallery.min.js"
                        ]
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "tsconfig.app.json",
                            "tsconfig.spec.json",
                            "e2e/tsconfig.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                },
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "e2e/protractor.conf.js",
                        "devServerTarget": "cadent:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "cadent:serve:production"
                        }
                    }
                }
            }
        }
    },
    "defaultProject": "cadent"
}